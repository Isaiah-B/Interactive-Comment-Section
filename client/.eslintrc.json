{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["warn", "single"],
        "linebreak-style": 1,
        "curly": ["error", "multi"],
        "nonblock-statement-body-position": ["warn", "below"],
        "no-underscore-dangle": "off",
        "no-unused-vars": "warn",
        "no-process-exit": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "func-names": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "no-use-before-define": "off"
    }
}
